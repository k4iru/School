@model IEnumerable<assign3_n01446543.Models.Student>
@{
    ViewBag.Title = "List";
}

<div class="container">
    <h2>List of Students</h2>
    <div class="row">
        <div class="col-sm-3">
            <h3>Student Name</h3>
        </div>
        <div class="col-sm-3">
            <h3>Student Number</h3>
        </div>
        <div class="col-sm-3">
            <h3>enrolDate</h3>
        </div>
    </div>
    @foreach (var student in Model)
    {
        <div class="row">
            <div class="col-sm-3"><p>@student.studentFname @student.studentLname</p></div>
            <div class="col-sm-3"><p>@student.studentNumber</p></div>
            <div class="col-sm-3"><p>@student.enrolDate.ToShortDateString()</p></div>

        </div>
    }

</div>
