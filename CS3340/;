import random
size = 1000003


class HashTable_LinearProbing:
    def __init__(self, array):
        self.array = array

    def hashFunc(self, key, value):
        hashed = abs(hash(key)) % size
        if self.array[hashed] == 'REMOVED' or self.array[hashed] == null:
            self.array[hashed] = value


class HashTable_DoubleHashing:
    def __init__(self, array):
        self.array = array

    def hashFunc(key, value):
        value - abs(hash(key)) % size
        return value


class _MapEntry:
    def __init__(self, hash_value, value):
        self.hash_value = hash_value
        self.value = value
